<h3>レビュー投稿</h3>

<%= form_with model: @review, url: reviews_path do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>

  <%= f.label :price %>
  <%= f.number_field :price %>

  <%= f.label :rating %>
  <%= f.number_field :rating %>

  <%= f.label :thought %>
  <%= f.text_area :thought %>

  <%= f.submit '投稿' %>
<% end %>
